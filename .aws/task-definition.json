{
  "family": "itorok-task",
  "executionRoleArn": "arn:aws:iam::568964983135:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "itorok-backend-container",
      "image": "568964983135.dkr.ecr.ap-northeast-2.amazonaws.com/itorok-backend:latest",
      "portMappings": [
        {
          "hostPort": 4000,
          "protocol": "tcp",
          "containerPort": 4000
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
